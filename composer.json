{
    "name": "antonioprimera/laravel-slack",
    "description": "A utility for posting messages to Slack",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio",
            "email": "antonio@cus.ro"
        }
    ],
    "require": {
        "antonioprimera/lara-packager": "^1.5",
        "spatie/laravel-package-tools": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "AntonioPrimera\\Slack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AntonioPrimera\\Slack\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "guzzlehttp/guzzle": "^7.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AntonioPrimera\\Slack\\LaravelSlackServiceProvider"
            ]
        }
    }
}
